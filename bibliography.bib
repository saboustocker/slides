@article{arzoglouRolePrivacyObstacles2023,
  title = {The {{Role}} of {{Privacy Obstacles}} in {{Privacy Paradox}}: {{A System Dynamics Analysis}}},
  shorttitle = {The {{Role}} of {{Privacy Obstacles}} in {{Privacy Paradox}}},
  author = {Arzoglou, Ektor and Kortesniemi, Yki and Ruutu, Sampsa and Elo, Tommi},
  date = {2023-04-19},
  journaltitle = {Systems},
  shortjournal = {Systems},
