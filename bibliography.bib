  shorttitle = {Taking the {{Load Off}} a {{Learner}}'s {{Mind}}},
  author = {family=Merrienboer, given=Jeroen J. G., prefix=van, useprefix=true and Kirschner, Paul A. and Kester, Liesbeth},
