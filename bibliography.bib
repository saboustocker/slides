  author = {Bombonato, Clara and Del Lucchese, Benedetta and Ruffini, Costanza and Di Lieto, Maria Chiara and Brovedani, Paola and Sgandurra, Giuseppina and Cioni, Giovanni and Pecini, Chiara},
