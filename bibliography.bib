@article{twohillPrimaryPreserviceTeachers2023,
  title = {Primary {{Preservice Teachers}}’ {{Mathematics Teaching Efficacy Beliefs}}: The {{Role Played}} by {{Mathematics Attainment}}, {{Educational Level}}, {{Preparedness}} to {{Teach}}, and {{Gender}}},
  shorttitle = {Primary {{Preservice Teachers}}’ {{Mathematics Teaching Efficacy Beliefs}}},
  author = {Twohill, Aisling and NicMhuirí, Siún and Harbison, Lorraine and Karakolidis, Anastasios},
  date = {2023-02},
  journaltitle = {International Journal of Science and Mathematics Education},
