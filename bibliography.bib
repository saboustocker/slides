@incollection{ryanEducationFlourishingSelfDetermination2023,
  title = {Education as {{Flourishing}}: {{Self-Determination Theory}} in {{Schools}} as {{They Are}} and as {{They Might Be}}},
  shorttitle = {Education as {{Flourishing}}},
  booktitle = {The {{Oxford Handbook}} of {{Self-Determination Theory}}},
