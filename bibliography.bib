@online{boeveSystematicEvaluationDutch2024,
  title = {A {{Systematic Evaluation}} of {{Dutch Large Language Models}}’ {{Surprisal Estimates}} in {{Sentence}}, {{Paragraph}}, and {{Book Reading}}},
