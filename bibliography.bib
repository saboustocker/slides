  title = {Privacy Concerns and Benefits in {{SaaS}} Adoption by Individual Users: {{A}} Trade-off Approach},
  shorttitle = {Privacy Concerns and Benefits in {{SaaS}} Adoption by Individual Users},
  author = {Gashami, Jean Pierre Guy and Chang, Younghoon and Rho, Jae Jeung and Park, Myeong-Cheol},
  date = {2016-09},
  journaltitle = {Information Development},
  shortjournal = {Information Development},
  volume = {32},
