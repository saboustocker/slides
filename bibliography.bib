@article{macleanExtendingBalloonAnalogue2018,
  title = {Extending the {{Balloon Analogue Risk Task}} to {{Assess Naturalistic Risk Taking}} via a {{Mobile Platform}}},
  author = {MacLean, R. Ross and Pincus, Aaron L. and Smyth, Joshua M. and Geier, Charles F. and Wilson, Stephen J.},
  date = {2018-03-01},
  journaltitle = {Journal of Psychopathology and Behavioral Assessment},
